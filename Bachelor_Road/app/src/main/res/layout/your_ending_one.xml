<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/prize"/>

    <Button
        android:id="@+id/ending_out_one"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#00000000"/>

    <TextView
        android:id="@+id/ending_name_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="23dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="陈涵"
        android:layout_marginLeft="160dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="158dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="23dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="一"
        android:layout_marginLeft="325dp"
        android:layout_marginStart="325dp"
        android:layout_marginTop="160dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="23dp"
        android:gravity="center"
        android:textStyle="bold"
        android:text="一"
        android:layout_marginLeft="462dp"
        android:layout_marginStart="462dp"
        android:layout_marginTop="160dp"
        />

    <TextView
        android:id="@+id/which_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:textColor="#fa0505"
        android:gravity="center"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:text="一等奖"
        android:layout_marginLeft="290dp"
        android:layout_marginStart="290dp"
        android:layout_marginTop="190dp"
        />


</RelativeLayout>
